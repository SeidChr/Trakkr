<Window x:Class="Trakkr.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:Trakkr.ViewModels"
        mc:Ignorable="d"
        Title="MainWindow" 
        Height="400" 
        Width="621.666"
        DataContext="{Binding MainViewModel}">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="63"/>
        </Grid.RowDefinitions>
        <Button x:Name="button" Content="Next" HorizontalAlignment="Left" Margin="10,0,0,6" Width="52" Height="49" VerticalAlignment="Bottom" Grid.Row="1" Command="{Binding Next}"/>
        <TextBox x:Name="textBox" Margin="67,0,10,34" TextWrapping="Wrap" Height="21" VerticalAlignment="Bottom" Grid.Row="1"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Margin="67,0,0,6" TextWrapping="Wrap" Width="157" Height="23" VerticalAlignment="Bottom" Grid.Row="1"/>
        <ComboBox x:Name="comboBox" Margin="229,0,10,6" Height="23" VerticalAlignment="Bottom" Grid.Row="1"/>
        <ListBox Grid.Row="0" x:Name="listBox" Margin="10,10,10,0" ItemsSource="{Binding EventCaptureSet.Events, Mode=OneWay}"/>

    </Grid>
</Window>

